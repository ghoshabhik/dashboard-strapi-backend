      "type": "string"
    },
    "GitHubUrl": {
      "type": "string"
    },
    "Completed": {
      "type": "boolean"
    },
    "StartDate": {
      "type": "date"
    },
    "EndDate": {
      "type": "date"
    },
    "Description": {
      "type": "text"
    }
  }
}
